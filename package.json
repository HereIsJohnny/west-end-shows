{
  "name": "west-end-shows",
  "version": "1.0.0",
  "description": "Application showing West End Shows",
  "main": "index.tsx",
  "scripts": {
    "precommit": "lint-staged && npm run typecheck && npm run test && npm run deps-lint",
    "start": "npm run develop",
    "develop": "webpack-dev-server --open",
    "build": "NODE_ENV=production webpack",
    "lint": "npm run tslint && npm run stylelint && npm run typecheck && npm run deps-lint",
    "typecheck": "tsc --noEmit",
    "tslint": "tslint './src/**/*.{ts,tsx}' --format stylish --force",
    "tslint-fix": "npm run tslint -- --fix",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "stylelint": "stylelint './src/**/*.ts*'",
    "format": "npm run prettier && npm run tslint-fix",
    "test": "jest"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "stylelint --fix",
      "prettier --write",
      "tslint --fix",
      "git add"
    ],
    "src/**/*.{js,jsx,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HereIsJohnny/west-end-shows.git"
  },
  "author": "Maciej Kowalski (maciej.kowalski.mk@gmail.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/HereIsJohnny/west-end-shows/issues"
  },
  "homepage": "https://github.com/HereIsJohnny/west-end-shows#readme",
  "dependencies": {
    "connected-react-router": "^6.4.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.8",
    "react-redux": "^7.0.3",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-observable": "^1.1.0",
    "rxjs": "^6.5.2",
    "slugify": "^1.3.4",
    "styled-components": "^4.1.3",
    "styled-reset": "^2.0.14",
    "typescript-fsa": "^3.0.0-beta-2",
    "typescript-fsa-reducers": "^1.2.1"
  },
  "devDependencies": {
    "@hot-loader/react-dom": "^16.8.6",
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.0.9",
    "@types/react-router": "^5.0.1",
    "@types/react-router-dom": "^4.3.3",
    "@types/styled-components": "^4.1.15",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^2.1.1",
    "express": "^4.17.1",
    "file-loader": "^3.0.1",
    "fork-ts-checker-notifier-webpack-plugin": "^1.0.0",
    "fork-ts-checker-webpack-plugin": "^1.3.4",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^5.17.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.1",
    "typescript-plugin-styled-components": "^1.4.3",
    "webpack": "^4.32.2",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.5.0",
    "webpack-merge": "^4.2.1"
  }
}
